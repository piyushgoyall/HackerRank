#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n,l,sum=0;
    scanf("%d", &n);
    //Complete the code to calculate the sum of the five digits on n.
    while(n>0)
    {
        l=n%10;
        sum=sum+l;
        n=n/10;
    }
    printf("%d",sum);
    return 0;
}
