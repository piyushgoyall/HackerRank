#include <iostream>
#include <string>
using namespace std;

int main()
{
    string a;
    string b;
    cin>>a;
    cin>>b;
    int l=a.size();
    int len=b.size();
    cout<<l<<" "<<len;
    cout<<"\n";
    string c=a+b;
    cout<<c;
    cout<<"\n";
    string temp;
    temp = a[0];
    a[0] = b[0];
    b[0] = temp[0];
    cout<<a<<" "<<b;
    
}
