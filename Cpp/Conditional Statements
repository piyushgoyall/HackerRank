#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    if(n==1)
    {
        cout<<"one";
        cout<<"\n";
    }
    else if(n==2)
    {
        cout<<"two";
        cout<<"\n";
    }
    else if(n==3)
    {
        cout<<"three";
        cout<<"\n";
    }
    else if(n==4)
    {
        cout<<"four";
        cout<<"\n";
    }
    else if(n==5)
    {
        cout<<"five";
        cout<<"\n";
    }
    else if(n==6)
    {
        cout<<"six";
        cout<<"\n";
    }
    else if(n==7)
    {
        cout<<"seven";
        cout<<"\n";
    }
    else if(n==8)
    {
        cout<<"eight";
        cout<<"\n";
    }
    else if(n==9)
    {
        cout<<"nine";
        cout<<"\n";
    }
    else 
    {
        cout<<"Greater than 9";
        cout<<"\n";
    }
}
