if __name__ == '__main__':
    s = input()
alnu = alpha = digit = lower = upper = False
for i in s:
    if i.isalnum():
        alnu = True
    if i.isalpha():
        alpha = True
    if i.isdigit():
        digit = True
    if i.islower():
        lower = True
    if i.isupper():
        upper = True
print(alnu)
print(alpha)
print(digit)
print(lower)
print(upper)
