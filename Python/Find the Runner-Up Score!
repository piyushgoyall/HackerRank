if __name__ == '__main__':
    n = int(input())
    arr = list(map(int, input().split()))
# m = max(arr)
# arr2 = []
# for i in arr:
#     if i != m:
#         arr2.append(i)
# print(max(arr2))

m = max(arr)
arr2 = []
for i in arr:
    if i == m:
        continue
    else:
        arr2.append(i)
print(max(arr2))
